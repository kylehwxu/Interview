package atoi;

public class Atoi {
	
	private static int solution(String s) {
		int index = 0;
		
		while (s.charAt(index) < '0' || s.charAt(index) > '9' || s.charAt(index) == ' ') {
			index++;
		}
		
		return -1;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
