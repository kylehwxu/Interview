package atoi;

public class Atoi {
	
	private static int solution(String s) {
		int index = 0;
		int sign = 1;
		int sum = 0;
		
		while (s.charAt(index) == ' ' && index < s.length()) {
			index++;
		}
		
		if (s.charAt(index) == '-') {
			sign = -1;
		}
		
		while (index < s.length()) {
			if (s.charAt(index) < '0' || s.charAt(index) > '9') {
				break;
			}
			
			index++;
		}
		
		
		
		return -1;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
